#include<stdio.h>
#include<string.h>
int main()
{
    char s1[100],s2[100];
    int k;
    printf("enter string 1");
    gets(s1);
    printf("enter string 2");
    gets(s2);
    k=strcmp;
    if(k==0)
    printf("strings are equal");
    if(k>0)
    printf("first string greater");
    if(k<0)
    printf("second string are greater");
    }
